CREATE VIEW BacalÃ¡ AS 
  SELECT title FROM
  (SELECT title, antu
  FROM 
  (SELECT title, COUNT(*) AS antu
    FROM taps_movies 
    WHERE pct > 50 and pct < 97
    GROUP BY title)
   ORDER BY antu DESC)
   WHERE ROWNUM <= 5;

