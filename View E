CREATE VIEW Bacal√° AS 
  SELECT title FROM
  (SELECT title, antu
  FROM 
  (SELECT title, COUNT(*) AS antu
    FROM taps_movies 
    WHERE pct BETWEEN 50 AND 97
    GROUP BY title)
   ORDER BY antu DESC)
   WHERE ROWNUM <= 5;

    --Poetic Justice
    --Evita
    --Spy Kids
    --Big Fat Liar
    --300
